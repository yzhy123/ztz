@CHARSET "UTF-8";

* {margin:0px;padding:0px;font-size:12px;font-family:'宋体';letter-spacing:0px;*letter-spacing:0.8px;}
div,ul,li,table,tr,td,img {margin:0 auto;border:0px;border:0px;}
h1,h2,h3,h4 {font-style:normal;font-weight:normal;}
