#index_menus {position:relative;height:45px;width:100%;margin:0 auto;}
#index_menus .menus {position:relative;height:45px;width:1002px;margin:0 auto;z-index:10;}
.category {position:relative;float:left;height:45px;width:200px;background:#5abb1e;z-index:10;}
#index_menus .category .mc {position:relative;left:0;width:198px;margin-top:45px;border:1px solid #CCC;/* display:none; */overflow:visible;z-index:13;}
#menus .category .mc {position:relative;left:0;width:198px;margin-top:45px;display:none;overflow:visible;z-index:13;}
#menus .category.hover .mc,#menus .category:hover .mc {display:block;}
.category h2 {float:left;height:45px;width:200px;background:url("../images/default.png") no-repeat 20px -65px;line-height:45px;font-size:16px;font-weight:bold;text-indent:40px;cursor:pointer;color:white;}
.category h2.hover,.category h2:hover {background:url("../images/default.png") no-repeat 20px -125px;}
.category .item {width:198px;height:70px;border:0;border-bottom:1px solid #CCC;}
.category .item.end {border-bottom:0;}
.category .item .i-mc {position:absolute;width:692px;left:198px;top:-1px;background:#fff;border:1px solid #CCC;display:none;overflow:hidden;}
.category .item span {position:absolute;height:70px;width:183px;padding-left:15px;background:white;filter:alpha(opacity=70);opacity:0.7;display:block;z-index:12;}
.category .item span .item_extra {float:left;height:24px;width:175px;padding-bottom:5px;line-height:24px;z-index:10;}
.category .item span .item_extra a {float:left;height:24px;margin-right:10px;line-height:24px;z-index:10;}
.category .item span .item_extra a.end {margin-right:0;}
.category .item h3 {float:left;height:36px;width:180px;font-weight:normal;line-height:36px;}
.category .item h3 a {float:left;top:0;margin-right:5px;font-weight:bold;}
.category .item h3 a.tag {float:right;font-weight:bold;font-family:"宋体";}
.category .item h3 a:hover,.category h3 a:active {color:#FF6600;}
.category .item dt {padding:3px 6px 0 0;font-weight:bold;color:#FF6600;}
.category .item dd {padding:3px 0 0;overflow:hidden;zoom:1;}
.category .subitem {float:left;width:464px;min-height:450px;padding:0 4px 0 8px;}
.category .subitem2 {float:left;width:330px;min-height:450px;padding:0 4px 0 8px;}
.category .subitem dl,.category .subitem2 dl {padding:6px 0;border-top:1px dotted #EED6B7;overflow:hidden;zoom:1;}
.category .subitem .firest,.category .subitem2 .firest {border-top:none;}
.category .subitem dt,.category .subitem2 dt {float:left;width:54px;line-height:22px;text-align:right;}
.category .subitem dd {float:left;width:402px;}
.category .subitem2 dd {float:left;width:260px;}
.category .subitem em,.category .subitem2 em {float:left;height:14px;margin:4px 0;line-height:14px;padding:0 8px;border-left:1px solid #ccc;font-style:normal;}
.category .subitem dt a,.category .subitem2 dt a {color:#FF6600;}
.category .subitem dd a,.category .subitem2 dd a {white-space:nowrap;}
.category .fr {width:210px;margin-bottom:-2000px;padding-bottom:2010px;background:#FCF4EA;}
.category .fr dl {width:194px;padding:6px 8px;}
.category .fr a {color:#666;}
.category .fr dd {padding-top:6px;line-height:20px;overflow:hidden;}
.category .categorys-brands li {float:left;width:97px;background:none;}
.category .categorys-promotions li {background:none;}
.category .hover span {width:184px;display:block;filter:alpha(opacity=100);opacity:1;z-index:13;}
.category .hover span .item_extra {border:transparent;}
.category .hover h3 {overflow:hidden;}
.category .hover .i-mc {display:block;z-index:12;}


